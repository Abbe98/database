PREFIX geo: <http://www.bigdata.com/rdf/geospatial#>

SELECT (COUNT(*) AS ?cnt) WHERE {
  SERVICE geo:search {
    ?res geo:search "inRectangle" .
    ?res geo:predicate <http://p> .
    ?res geo:spatialRectangleUpperLeft "0#-178" .
    ?res geo:spatialRectangleLowerRight "0#178" .
    ?res geo:timeStart "0" .
    ?res geo:timeEnd "0" .
  }
} 